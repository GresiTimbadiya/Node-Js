<%- include('header') %>


    <div class="page-wrapper">

        <div class="page-breadcrumb">
            <div class="row">
                <div class="col-12 d-flex no-block align-items-center">
                    <h4 class="page-title">Admin</h4>
                    <div class="ml-auto text-right">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Library</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row justify-content-center">
                <div class="col-md-9">
                    <div class="card">
                        <form class="form-horizontal" method="post" enctype="multipart/form-data"
                            action="/admin/addAdminData">
                            <div class="card-body pb-0">
                                <h4 class="card-title">Admin Info</h4>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label for="fname"
                                                class="col-sm-4 text-right control-label col-form-label">First
                                                Name</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" name="fname" id="fname"
                                                    placeholder="First Name Here">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label for="lname"
                                                class="col-sm-4 text-right control-label col-form-label">Last
                                                Name</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" name="lname" id="lname"
                                                    placeholder="Last Name Here">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label
                                                class="col-sm-4 text-right control-label col-form-label">Email</label>
                                            <div class="col-sm-8">
                                                <input type="email" class="form-control" name="email"
                                                    placeholder="Email Here">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label
                                                class="col-sm-4 text-right control-label col-form-label">Password</label>
                                            <div class="col-sm-8">
                                                <input type="password" name="password" class="form-control"
                                                    placeholder="Password Here">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="col-sm-3 pr-2">Gender</label>
                                            <div class="col-sm-9">
                                                <div class="custom-control custom-radio">
                                                    <input type="radio" class="custom-control-input"
                                                        id="customControlValidation1" value="Male" name="gender"
                                                        required>
                                                    <label class="custom-control-label"
                                                        for="customControlValidation1">Male</label>
                                                </div>
                                                <div class="custom-control custom-radio">
                                                    <input type="radio" class="custom-control-input"
                                                        id="customControlValidation2" value="Female" name="gender"
                                                        required>
                                                    <label class="custom-control-label"
                                                        for="customControlValidation2">Female</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="col-sm-4">Qualification</label>
                                            <div class="col-sm-8">
                                                <div class="d-flex">
                                                    <div class="custom-control custom-checkbox mr-sm-2">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="customControlAutosizing1" value="10th"
                                                            name="qualification">
                                                        <label class="custom-control-label"
                                                            for="customControlAutosizing1">10th</label>
                                                    </div>
                                                    <div class="custom-control custom-checkbox mr-sm-2">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="customControlAutosizing2" name="qualification"
                                                            value="12th">
                                                        <label class="custom-control-label"
                                                            for="customControlAutosizing2">12th</label>
                                                    </div>
                                                    <div class="custom-control custom-checkbox mr-sm-2">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="customControlAutosizing3" name="qualification"
                                                            value="Graduation">
                                                        <label class="custom-control-label"
                                                            for="customControlAutosizing3">Graduation</label>
                                                    </div>
                                                </div>
                                                <div class="custom-control custom-checkbox mr-sm-2">
                                                    <input type="checkbox" class="custom-control-input"
                                                        id="customControlAutosizing4" name="qualification"
                                                        value="Diploma">
                                                    <label class="custom-control-label"
                                                        for="customControlAutosizing4">Diploma</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row  align-items-center d-flex">
                                            <% var
                                                city=["Delhi","Mumbai","Pune","Kolkata","Chennai","Hyderabad","Bangalore"]
                                                %>
                                                <label class="col-sm-2 pr-0 m-t-15">City</label>
                                                <div class="col-sm-7">
                                                    <select name="city" class="select2 form-control custom-select"
                                                        style="width: 100%; height:36px;">
                                                        <option>--Select city--</option>
                                                        <% city.map((val)=>{ %>
                                                            <option value="<%= val %>">
                                                                <%= val %>
                                                            </option>
                                                            <% }) %>
                                                    </select>
                                                </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label for="cono1"
                                                class="col-sm-4 text-right control-label col-form-label">Message</label>
                                            <div class="col-sm-8">
                                                <textarea class="form-control" name="message"
                                                    placeholder="Write your message"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="col-sm-4">File Upload</label>
                                            <div class="col-sm-8">
                                                <div class="custom-file">
                                                    <input type="file" name="profile" class="custom-file-input"
                                                        id="validatedCustomFile" required>
                                                    <label class="custom-file-label" for="validatedCustomFile">Choose
                                                        file...</label>
                                                    <div class="invalid-feedback">Example invalid custom file feedback
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="border-top">
                                <div class="card-body">
                                    <button type="submit" class="btn btn-primary">Submit</button>
                                </div>
                            </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>

        <%- include('footer') %>